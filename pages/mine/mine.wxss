.mine .userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-bottom: 60rpx;
    border-bottom: 1px solid #ccc;
    box-sizing: border-box;
  }
  
  .mine .userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
  }
  
  .mine .userinfo-nickname {
    color: #aaa;
  }
  